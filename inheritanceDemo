		krediUI.KrediHesapla(new OgretmenKrediManager());
		krediUI.KrediHesapla(new TarimKrediManager());
		krediUI.KrediHesapla(new BaseKrediManager());
		krediUI.KrediHesapla(new AskerKredisiManager());

	}

}


